{
  "author": "Jonas Peeck <jonas.peeck@googlemail.com> (https://twitter.com/aguynamedjonas)",
  "name": "slim-redux-react",
  "description": "Official react bindings for slim-redux",
  "keywords": [
    "react",
    "reactjs",
    "redux",
    "flux",
    "state",
    "fsa"
  ],
  "version": "0.0.10",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/aGuyNamedJonas/slim-redux-react.git"
  },
  "main": "main.js",
  "jsnext:main": "main.es.js",
  "babel": {
    "presets": [
      "latest",
      "stage-0",
      "react"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb-base"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "react": "^15.4.2",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0",
    "reselect": "^3.0.0",
    "slim-redux": "^0.2.4"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^20.0.3",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.16.3",
    "chai": "^4.0.0-canary.1",
    "coveralls": "^2.11.14",
    "del": "^2.2.2",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^3.8.0",
    "eslint-config-airbnb-base": "^10.0.1",
    "eslint-plugin-import": "^2.2.0",
    "istanbul": "^1.1.0-alpha.1",
    "jest": "^20.0.4",
    "mocha": "^3.1.2",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "rollup": "^0.36.3",
    "rollup-plugin-babel": "^2.6.1",
    "sinon": "^2.0.0-pre.3"
  },
  "scripts": {
    "lint": "eslint src test tools",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cover": "babel-node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "build": "node tools/build",
    "prepublish": "npm run build",
    "publish:docs": "easystatic deploy docs --repo kriasoft/babel-starter-kit",
    "start": "easystatic start docs"
  },
  "jest": {
    "verbose": true
  }
}
